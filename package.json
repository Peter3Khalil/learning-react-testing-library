{
  "name": "learning-react-testing-library",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "set NODE_ENV=dev && next dev -p 7000",
    "build": "set NODE_ENV=prod && next build",
    "start": "set NODE_ENV=prod && next start -p 7000",
    "test": "jest --watch",
    "lint:fix": "eslint src --fix",
    "lint": "eslint src",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run format && npm run lint:fix && npm run type-check"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "lucide-react": "^0.314.0",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.3.3"
  }
}
